{
  "name": "qg-gameserver",
  "version": "1.0.0",
  "main": "main.ts",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && webpack --mode development",
    "watch": "rimraf dist && webpack --mode development --watch",
    "pretty": "npx prettier \"*/**/*.ts*\"  --config .prettierrc --write",
    "lint": "npx tslint -p tsconfig.json --fix",
    "start": "node ./dist/index.js",
    "start-local": "node ./index.js",
    "debug": "node --inspect-brk ./dist/index.js",
    "build-prod": "rimraf dist && webpack --mode production  --progress --hide-modules"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/ws": "^7.2.6",
    "@types/redis": "^2.8.27",
    "@types/node-fetch": "^2.5.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "13.9.2",
    "prettier": "^2.0.2",
    "webpack-node-externals": "^1.7.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.7.0",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "copy-webpack-plugin": "^5.1.1"
  }
}
